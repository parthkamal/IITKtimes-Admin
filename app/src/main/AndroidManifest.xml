<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.parth.iitktimes">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.IITKTimes">
        <activity android:name=".Notice"></activity>
        <activity android:name=".addBooks" />
        <activity android:name=".updataDelete" />
        <activity android:name=".creators.updateCreators" />
        <activity android:name=".creators.addCreators" />
        <activity
            android:name=".AddImages"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".AddNotice"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>